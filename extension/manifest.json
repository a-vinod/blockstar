{
  "name": "BlockStar",
  "version": "1.0",
  "manifest_version": 2,
  "description": "BlockStar Review Score",
  "browser_action": {
    "default_icon": {
      "128": "icon-128.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": [
  "contextMenus",
  "tabs"
  ],
  "background": {
    "scripts": [
    "js/background.js"
    ]
  }
}